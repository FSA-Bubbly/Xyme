    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "css-loader": "^6.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "konva": "^8.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "react-use": "^17.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
